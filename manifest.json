{
    "name": "Website Blocker",
    "description": "A simple extension that blocks websites",
    "version": "0.0.2",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "action": {
        "default_popup": "popup.html"
      }
}